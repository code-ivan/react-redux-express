{
  "name": "rangewell-react",
  "version": "2.0.0",
  "description": "Step by step creation of a Universal React + Redux Boilerplate",
  "main": "index.js",
  "scripts": {
    "dev": "node --trace-warnings ./bin/server",
    "dev-debug": "node --inspect --debug-brk --trace-warnings ./bin/server",
    "start": "node ./bin/server",
    "start-prod": "cross-env NODE_ENV=production node ./bin/server",
    "postinstall": "babel src --out-dir lib --copy-files && webpack --progress",
    "build": "npm run build:server && npm run build:client",
    "build:server": "cross-env NODE_ENV=production babel src --out-dir lib --copy-files",
    "build:client": "cross-env NODE_ENV=production webpack --progress",
    "clean": "rimraf dist lib",
    "check": "npm run lint && npm run test && npm run flow",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint --cache .",
    "lint:css": "stylelint src/**/*.scss --syntax scss",
    "test": "cross-env NODE_ENV=test mocha mocha.config.js --timeout 35000",
    "flow": "flow",
    "flow:stop": "flow stop"
  },
  "repository": {
    "type": "git",
    "url": "https://rangewell.visualstudio.com/Rangewell/_git/RangewellReact"
  },
  "keywords": [
    "isomorphic",
    "universal",
    "react",
    "redux",
    "boilerplate"
  ],
  "author": "Rangewell",
  "license": "CC0-1.0",
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "autoprefixer": "^7.1.2",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-node5": "^1.2.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "bluebird": "^3.5.0",
    "body-parser": "^1.18.1",
    "classnames": "^2.2.5",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.4",
    "express": "^4.15.4",
    "extract-text-webpack-plugin": "^2.1.2",
    "isomorphic-fetch": "^2.2.1",
    "linq": "^3.0.9",
    "moment": "^2.18.1",
    "mongodb": "^2.2.31",
    "mongoose": "^4.11.9",
    "node-sass": "^4.9.0",
    "postcss-loader": "^2.0.6",
    "prop-types": "^15.5.10",
    "react": "^15.6.2",
    "react-day-picker": "^6.1.0",
    "react-dom": "^15.6.2",
    "react-helmet": "^5.1.3",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-intl": "^2.4.0",
    "react-native": "^0.47.2",
    "react-player": "^0.24.5",
    "react-redux": "^5.0.5",
    "react-render-html": "^0.5.0",
    "react-router-config": "^1.0.0-beta.3",
    "react-router-dom": "^4.1.1",
    "react-scroll": "^1.5.4",
    "react-select": "^1.1.0",
    "react-slider": "^0.9.0",
    "react-validation": "^2.10.9",
    "redux": "^3.7.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.1.0",
    "webpack-dev-middleware": "^1.11.0",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-isomorphic-tools": "^3.0.3"
  },
  "devDependencies": {
    "chai": "^4.0.2",
    "chai-as-promised": "^7.1.1",
    "chai-enzyme": "^0.8.0",
    "css-modules-require-hook": "^4.0.6",
    "enzyme": "^2.9.1",
    "eslint": "^4.1.1",
    "eslint-plugin-flowtype": "^2.34.1",
    "eslint-plugin-react": "^7.1.0",
    "flow-bin": "^0.49.1",
    "glob": "^7.1.2",
    "jsdom": "^11.1.0",
    "mocha": "^3.4.2",
    "pretty-cli": "0.0.14",
    "react-test-renderer": "^15.6.1",
    "rimraf": "^2.6.1",
    "sinon": "^2.3.6",
    "sinon-chai": "^2.11.0",
    "stylelint": "^7.12.0"
  }
}
